.obj/Application.o \
.obj/Client.o \
.obj/EditWindow.o \
.obj/main.o \


ASM_LIST = \
.obj/App.s \
.obj/Application.s \
.obj/Client.s \
.obj/EditWindow.s \
.obj/main.s \


DEP_LIST = \
.obj/App.dep \
